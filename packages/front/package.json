{
  "name": "front",
  "version": "0.3.1",
  "description": "A Quasar Framework app",
  "productName": "Open For Travel Guide",
  "author": "chetzof <vcosvic@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m ssr",
    "lint": "eslint . --ext .js,.vue,.ts",
    "format": "prettier --write  .",
    "fix": "yarn lint --fix && yarn format",
    "test": "echo \"No test specified\" && exit 0",
    "fake-server": "json-server --watch db.json",
    "typecheck": "tsc -p tsconfig.json",
    "build": "quasar build -m ssr && cp app.yaml dist/ssr/app.yaml",
    "serve-build": "cd dist/ssr && npm start",
    "build-serve": "quasar build -m ssr && cd dist/ssr && npm start",
    "serve": "cd dist/ssr && GOOGLE_APPLICATION_CREDENTIALS=\"../../gcloud/dev-serviceaccount.json\" npm start",
    "build-deploy": "yarn run build && yarn run deploy",
    "deploy": "gcloud app deploy dist/ssr/app.yaml --quiet",
    "build-flags": "ts-node src/bin/build-flags.ts",
    "build-sitemap": "rm -rf var/sitemap && node -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register src/bin/build-sitemap.ts",
    "build-stats": "node -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register ../functions/src/safety-level-calculator.ts",
    "upload-assets": "cd dist/ssr && CLOUDSDK_PYTHON=/usr/bin/python2.7 gsutil -m cp -r -z css,html,js,map,svg ./www/** gs://oftassets",
    "delete-translation-cache": "gsutil -m rm -f gs://translation-cache/** "
  },
  "workspaces": {
    "nohoist": [
      "firebase",
      "firebase/**",
      "grpc/**",
      "@vueuse/core",
      "ky",
      "ky/*",
      "ky-universal",
      "encoding",
      "node-fetch",
      "webpack-filter-warnings-plugin"
    ]
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.13",
    "@amcharts/amcharts4-geodata": "^4.1.19",
    "@google-cloud/profiler": "^4.1.0",
    "@google-cloud/secret-manager": "^3.2.3",
    "@google-cloud/storage": "^5.7.0",
    "@quasar/extras": "^1.0.0",
    "@types/lru-cache": "^5.1.0",
    "@types/node-fetch": "^2.5.7",
    "@vue/composition-api": "^0.6.4",
    "@vueuse/core": "^4.0.0-rc.9",
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "countup.js": "^2.0.7",
    "date-fns": "^2.16.1",
    "firebase": "^8.2.0",
    "i18n-iso-countries": "^6.0.0",
    "i18n-nationality": "^1.1.1",
    "iso-language-list": "^0.3.3",
    "ky": "^0.25.1",
    "ky-universal": "^0.8.2",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "portal-vue": "^2.1.7",
    "quasar": "^1.0.0",
    "rfs": "^9.0.3",
    "svg-country-flags": "^1.2.7",
    "typescript": "^4.1.3",
    "vue-auto-i18n": "1.1.0-beta.13",
    "vue-countup-v2": "^4.0.0",
    "vue-i18n": "^8.0.0",
    "vue-inline-svg": "^2.0.0",
    "vue-lazy-hydration": "^2.0.0-beta.4",
    "@google-cloud/debug-agent": "^5.1.3",
    "bunyan": "^2.0.5",
    "@google-cloud/logging-bunyan": "^2.0.3",
    "vue-social-sharing": "^3.0.5"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.14.14",
    "@types/sharp": "^0.27.1",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^27.0.0",
    "eslint-plugin-unused-imports": "^1.0.0",
    "eslint-plugin-vue": "^7.2.0",
    "filesize": "^6.1.0",
    "md5-file": "^5.0.0",
    "prettier": "^2.1.1",
    "shared": "0.3.1",
    "sharp": "^0.27.1",
    "sitemap": "^6.3.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "vue-svg-inline-loader": "^2.0.0"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 2 Edge versions",
    "last 3 Safari versions",
    "last 3 Android versions",
    "last 3 ChromeAndroid versions",
    "last 3 FirefoxAndroid versions",
    "last 3 iOS versions"
  ],
  "engines": {
    "node": "14.x.x",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
