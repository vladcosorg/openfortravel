{
  "name": "front",
  "version": "1.18.0",
  "description": "A Quasar Framework app",
  "productName": "Open For Travel Guide",
  "author": "chetzof <vcosvic@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m ssr",
    "format": "yarn run -T prettier --write  .",
    "lint": "yarn run -T _lint packages/front",
    "fix": "yarn format && yarn lint",
    "fake-server": "json-server --watch db.json",
    "typecheck": "tsc -p tsconfig.json",
    "build": "quasar build -m ssr && cp app.yaml dist/ssr/app.yaml",
    "serve-build": "cd dist/ssr && npm start",
    "build-serve": "quasar build -m ssr && cd dist/ssr && npm start",
    "serve": "cd dist/ssr && GOOGLE_APPLICATION_CREDENTIALS=\"../../gcloud/dev-serviceaccount.json\" npm start",
    "build-deploy": "yarn run build && yarn run deploy",
    "deploy": "cp app.yaml dist/ssr/app.yaml && gcloud app deploy dist/ssr/app.yaml --quiet",
    "build-flags": "ts-node src/bin/build-flags.ts",
    "build-sitemap": "rm -rf var/sitemap && yarn run -T ts-node -r tsconfig-paths/register  src/bin/build-sitemap.ts",
    "build-stats": "node -- node_modules/.bin/ts-node -P tsconfig.json -r tsconfig-paths/register ../functions/src/stats-aggregator.ts",
    "upload-assets": "cd dist/ssr && CLOUDSDK_PYTHON=/usr/bin/python2.7 gsutil -m cp -r -z css,html,js,map,svg ./www/** gs://oftassets",
    "delete-translation-cache": "gsutil -m rm -f gs://translation-cache/** ",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.21",
    "@amcharts/amcharts4-geodata": "^4.1.21",
    "@cospired/i18n-iso-languages": "^3.0.0",
    "@vueuse/core": "^5.3.0",
    "countries-list": "^2.6.1",
    "firebase": "9.0.0-beta.8",
    "i18n-iso-countries": "^6.8.0",
    "i18n-nationality": "^1.1.1",
    "iso-language-list": "^0.3.3",
    "ky": "^0.25.1",
    "ky-universal": "^0.8.2",
    "lodash": "^4.17.21",
    "quasar": "^2.0.3",
    "rfs": "^9.0.5",
    "svg-country-flags": "^1.2.10",
    "vue-auto-i18n": "1.1.0-beta.19",
    "vue-i18n": "^9.2.0-beta.2",
    "vue-inline-svg": "^3.0.0-beta.3",
    "vue-social-sharing": "^3.0.8",
    "vuex": "^4.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@quasar/app": "^3.0.4",
    "@quasar/extras": "^1.10.10",
    "@quasar/quasar-app-extension-testing": "^1.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^2.2.2",
    "@types/lodash": "^4.14.172",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^14.17.9",
    "@types/sharp": "^0.28.5",
    "@types/webpack-env": "^1.16.2",
    "@types/winston": "^2.4.4",
    "filesize": "^7.0.0",
    "md5-file": "^5.0.0",
    "sharp": "^0.28.3",
    "sitemap": "^7.0.0",
    "tsconfig-paths": "^3.10.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "vue-svg-inline-loader": "^2.1.2"
  },
  "browserslist": [
    "last 5 Chrome versions",
    "last 5 Firefox versions",
    "last 2 Edge versions",
    "last 3 Safari versions",
    "last 3 Android versions",
    "last 3 ChromeAndroid versions",
    "last 3 FirefoxAndroid versions",
    "last 3 iOS versions"
  ],
  "engines": {
    "node": "14.x.x",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.18.0"
  }
}
